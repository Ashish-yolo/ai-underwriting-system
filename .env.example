# Application
NODE_ENV=development
PORT=3000
FRONTEND_URL=http://localhost:5173

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/underwriting
MONGODB_URL=mongodb://localhost:27017/underwriting

# Redis
REDIS_URL=redis://localhost:6379

# Security
JWT_SECRET=your-jwt-secret-change-this-in-production
ENCRYPTION_KEY=your-32-byte-hex-encryption-key-change-this
WEBHOOK_SECRET=your-webhook-secret-change-this

# LOS Integration
LOS_WEBHOOK_URL=https://new-age-los.netlify.app/api/underwriting/callback

# External APIs (Configure as needed)
CIBIL_API_URL=
CIBIL_API_KEY=
EXPERIAN_API_URL=
EXPERIAN_API_KEY=
EQUIFAX_API_URL=
EQUIFAX_API_KEY=
NSDL_PAN_API_URL=
NSDL_API_KEY=
AADHAAR_API_URL=
AADHAAR_API_KEY=

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
SMTP_FROM=noreply@underwriting.com

# Slack Integration (for alerts)
SLACK_WEBHOOK_URL=

# File Upload
MAX_FILE_SIZE_MB=10
ALLOWED_FILE_TYPES=.xlsx,.xls,.docx,.json,.csv

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Session
SESSION_SECRET=your-session-secret-change-this
SESSION_TIMEOUT_HOURS=24

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs/app.log

# Feature Flags
ENABLE_ASYNC_PROCESSING=true
ENABLE_WEBHOOK_RETRIES=true
ENABLE_ANALYTICS=true
